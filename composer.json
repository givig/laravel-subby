{
    "name": "bpuig/laravel-subby",
    "description": "Laravel Subby is a flexible plans and subscription management system for Laravel.",
    "type": "library",
    "keywords": [
        "plan",
        "value",
        "laravel",
        "feature",
        "database",
        "recurring",
        "subscription"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bpuig/laravel-subby/issues",
        "source": "https://github.com/bpuig/laravel-subby",
        "docs": "https://github.com/bpuig/laravel-subby/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Borja Puig"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^v11.0",
        "doctrine/dbal": "^4.0"
    },
    "require-dev": {
        "illuminate/console": "^10.0|^11.0",
        "illuminate/container": "^10.0|^11.0",
        "phpunit/phpunit": "^9.5.8|^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bpuig\\Subby\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bpuig\\Subby\\Tests\\": "tests",
            "Bpuig\\Subby\\Tests\\Database\\Factories\\": "tests/Database/Factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bpuig\\Subby\\SubbyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
